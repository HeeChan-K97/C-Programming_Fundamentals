ex1-page.155
#include <stdio.h>

int main()
{
	int num;
	int selectnum;
	
	printf("양의 정수 입력: ");
	scanf("%d", &num);

	while(selectnum<num)
	{
		printf("Hello World! \n");
		selectnum++;
	}
	return 0;
}
ex2-page.155
# include <stdio.h>

int main(void)
{
    int num=0;
    int cnt=0;

    printf("Number of multiples of 3: ");
    scanf("%d", &num);

    while(cnt++<num)
    
    	printf("%d ", 3*cnt);
    
    return 0;
} 
